{% macro AttrFav(attn, attv, fav) %}{% set c='' %}{# --------------------------------- #}
<span><input value="{{attv}}" class="form-control input-small r" id="{{attn}}" name="{{attn}}" type="number" /></span>
<input value="0" id="{{attn}}_fav" name="{{attn}}_fav" type="hidden" />
<input value="1" {% if fav == 1 %} checked="checked"{% endif %} id="{{attn}}_fav" name="{{attn}}_fav" type="checkbox" title="Favoured Attribute" />
{% endmacro AttrFav %}
{% macro genSkill(id, name, rankname, bonus, tn) %}{# --------------------------------- #}
{% set eo = "od" %}{% if id is even %}{% set eo = "ev" %}{% endif %}
{% if bonus == '' %}{% set bonus = 0 %}{% endif %}
{% if tn == '' %}{% set tn = 0 %}{% endif %}
<tr><td class="lbl" title="Skill: {{name}}"><input value="{{name}}" class="form-control" id="skill_name_{{id}}" name="skill_name_{{id}}" type="text" /></td>
<td class="{{eo}} txt c" title="Rank: {{rankname}}">
<select class="form-control" id="skill_rank_{{id}}" name="skill_rank_{{id}}">
    <option value="Novice (0)" {% if rankname is defined and rankname == "Novice (0)" %}selected="selected" {% endif %} >Novice (0)</option>
    <option value="Apprentice (10)" {% if rankname is defined and rankname == "Apprentice (10)" %}selected="selected" {% endif %} >Apprentice (10)</option>
    <option value="Journeyman (20)" {% if rankname is defined and rankname == "Journeyman (20)" %}selected="selected" {% endif %} >Journeyman (20)</option>
    <option value="Adept (30)" {% if rankname is defined and rankname == "Adept (30)" %}selected="selected" {% endif %} >Adept (30)</option>
    <option value="Expert (40)" {% if rankname is defined and rankname == "Expert (40)" %}selected="selected" {% endif %} >Expert (40)</option>
    <option value="Master (50)" {% if rankname is defined and rankname == "Master (50)" %}selected="selected" {% endif %} >Master (50)</option>
</select>
</td>
<td class="{{eo}} num r" title="Bonus: only add bonus for specializations!"><input value="{{bonus is defined ? bonus }}" class="form-control input-small r" id="skill_bonus_{{id}}" name="skill_bonus_{{id}}" type="number" /></td>
<td class="{{eo}} num r" title="Base Target Number: {{tn}}"><input value="{{tn}}" class="form-control input-small r" id="skill_base_tn_{{id}}" name="skill_base_tn_{{id}}" type="number" /></td></tr>{% endmacro genSkill %}
{% macro genWeapon(id, name, damage, size, reach, qual) %}{# --------------------------------- #}
<tr><td class="att hdr c" title="Handhold, eg (1H)and, (1.5H)and, (2H)and"><input value="{{size}}" class="form-control input-small c" id="weapon_hand_{{id}}" name="weapon_hand_{{id}}" type="text" /></td>
    <td class="att hdr c" colspan="2" title="Name of the weapon">Name: <input value="{{name}}" class="form-control" id="weapon_name_{{id}}" name="weapon_name_{{id}}" type="text" /></td>
</tr><tr>
    <td class="hdr c">Damage</td>
    <td class="hdr c">Range/Reach</td>
    <td class="hdr c">Qualities</td>
</tr><tr>
    <td class="txt c" title="Damage, eg 1d4, 1d6, 1d10"><input value="{{damage}}" class="form-control input-small c" id="weapon_dmg_{{id}}" name="weapon_dmg_{{id}}" type="text" /></td>
    <td class="txt c" title="Reach, eg 1m, 2m, 3m or Range, eg 10/15/20"><input value="{{reach}}" class="form-control input-small c" id="weapon_reach_{{id}}" name="weapon_reach_{{id}}" type="text" /></td>
    <td class="txt l" ><input value="{{qual}}" class="form-control" id="weapon_qual_{{id}}" name="weapon_qual_{{id}}" type="text" /></td>
</tr>{% endmacro genWeapon %}
{% macro genSpell(id, name, level, cost, strength, desc) %}{# --------------------------------- #}
<tr><td class="att hdr c" colspan="6" title="Name of the spell">Spell Name: <input value="{{name}}" class="form-control" id="spell_name_{{id}}" name="spell_name_{{id}}" type="text" /></td></tr>
<tr><td class="hdr c">Lvl</td>
    <td class="txt c" title="Level of the spell, eg 1, 2, 3, 4"><input value="{{level}}" class="form-control input-small" id="spell_level_{{id}}" name="spell_level_{{id}}" type="number" /></td>
    <td class="hdr c">Strength</td>
    <td class="txt c" title="Strength of the spell, eg 4, 6, [roll:1d10]"><input value="{{strength}}" class="form-control input-small" id="spell_strength_{{id}}" name="spell_strength_{{id}}" type="text" /></td>
    <td class="hdr c">Cost</td>
    <td class="txt c" title="Cost of the spell, eg 1, 2, 3, 4"><input value="{{cost}}" class="form-control input-small" id="spell_cost_{{id}}" name="spell_cost_{{id}}" type="number" /></td>
</tr><tr>
    <td class="hdr c" colspan="2">Description</td>
    <td class="txt l" colspan="4"><textarea class="form-control" id="spell_desc_{{id}}" name="spell_desc_{{id}}" rows="5">{{desc}}</textarea></td>
</tr>{% endmacro genSpell %}
{% macro genTalent(id, name, desc) %}{# --------------------------------- #}
<tr><td class="att hdr c" title="Name of the talent">Name: <input value="{{name}}" class="form-control" id="talent_name_{{id}}" name="talent_name_{{id}}" type="text" /></td></tr>
<tr><td class="txt l"><textarea class="form-control" id="talent_desc_{{id}}" name="talent_desc_{{id}}" rows="5">{{desc}}</textarea></td></tr>{% endmacro genTalent %}
{% macro genTrait(id, name, desc) %}{# --------------------------------- #}
<tr><td class="att hdr c" title="Name of the trait">Name: <input value="{{name}}" class="form-control" id="trait_name_{{id}}" name="trait_name_{{id}}" type="text" /></td></tr>
<tr><td class="txt l"><textarea class="form-control" id="trait_desc_{{id}}" name="trait_desc_{{id}}" rows="5">{{desc}}</textarea></td></tr>{% endmacro genTrait %}
